package basic_P;

public class ThreeSixNine2 {
	public static void main(String[] args) {
		int n = 20;
		
		for(int i=1; i<=n; i++){
			// int[] arr = new int[i];
			int a = i%10;
			int b = a%3;
			
			try {
				int c = i/b;
			}catch(ArithmeticException e) {
				System.out.println(i);
			}
		}
		
	}
}
