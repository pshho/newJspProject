package basic_P;

public class Person extends Major{
	private String name;
	private int sum, avg, rank, kor, eng, math;
	
	public Person(String name, String... majorName) {
		super(majorName);
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

}
