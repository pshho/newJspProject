package projClass;

public class Star {

	public static void main(String[] args) {
		for(int i=0; i<5; i++) {
			for(double j=0.0; j<(16-i)/2; j += 1) {
				if(j==7 && i==0) {
					j+=0.5;
				}
				//System.out.print(" ");
				System.out.print(j);
			}
			
			for(double j=0.0; j<i+1; j += 1) {
				System.out.print("*");
				// System.out.print("ë‘");
			}
			
			System.out.println();
		}
	}

}
