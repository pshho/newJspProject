package projClass;

import java.util.List;

public class Person extends Major{
	private String name;	// 사람
	private List<Major> majors;	// 과목 객체(점수 포함)
	private String grade;

	public Person(String name, Major major, String grade) {
		this.name = name;
		
		for(Major maj : major) {
			this.majors.add(maj);
			this.majors.add(grade);
		}
	}

}
