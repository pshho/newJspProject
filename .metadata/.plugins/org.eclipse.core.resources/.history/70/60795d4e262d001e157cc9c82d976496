package arr_p;

public class SortMain {

	public static void main(String[] args) {
		// 77,65,98,46,82
		// 46,65,77,82,98
		int a = 10;
		int b = 20;
		
		System.out.println(a+", "+b);
		int buf = b;
		b = a;
		a = buf;
		
		System.out.println(a+", "+b);
		
		int[] q = {77,65,98,46,82};
		for(int i=0; i<q.length; i++) {
			for(int j=i+1; j<=q.length; j++) {
				if(q[i]<q[j]) {
					int z = q[i];
					q[i] = q[j];
					q[j] = z;
				}
			}
		}
	}

}
