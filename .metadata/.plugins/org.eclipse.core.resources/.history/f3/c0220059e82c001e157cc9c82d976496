public class Person extends Major{
	private String name;	// 사람
	private List<Major> majors;	// 과목 객체(점수 포함)
	private String[] grades;
	private Map<major, value> testMap;
	
	public Person(String name, String... grades) {
		this.name = name;
		this.majors = new ArrayList<>();
		
		for(int i=0; i<major.length; i++) {
			this.majors.add();
		}
	}

}
