package JspProject;

public class People extends Subject{ // 과목(총점, 평균, 등수, 등급) 배열 상속받은 사람 클래스
	private String name; // 사람
	private int names; // 가나다 순서를 정렬할 배열

	// 사람과 과목은 1대 다의 관계
	public People(String name, String... subjects) {
		super(subjects);
		this.name = name;
	}
	
	public People() {};
	
	// 초기 입력 받은 사람들의 이름 데이터
	public void setNames(int names) {
		this.names = names;
	}
	
	public String getName() {
		return name;
	}
	
}
