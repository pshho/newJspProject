package basic_P;

public class StudentTest {

	public static void main(String[] args) {
		Person person1 = new Person("강남", "국어", "영어", "수학");
		Person person2 = new Person("강남", "국어", "영어", "수학", "과학");
		Person person3 = new Person("강남", "국어", "영어", "수학", "과학", "음악");
		
		// System.out.println(person1.getName());
		// System.out.println(person2.getName());
		// System.out.println(person3.getName());
		
		for (String pm : person1.getMajor()) {
			System.out.println(pm);
		}
		
		System.out.println();
		
		for (String pm : person2.getMajor()) {
			System.out.println(pm);
		}
		
		System.out.println();
		
		for (String pm : person3.getMajor()) {
			System.out.println(pm);
		}

	}

}
