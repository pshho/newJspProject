package exercise;

public class StarMain {

	public static void main(String[] args) {
		for(int i=1; i<6; i++) {
//			for(int j=1; j<20; j += 1) {
//				// System.out.print(" ");
//				System.out.print(j);
//			}
			
			for(int j=0; j<i+1; j += 1) {
				System.out.print("*");
				if(i==1) {
					break;
				}
				// System.out.print("두");
			}
			
			System.out.println();
		}
	}

}
