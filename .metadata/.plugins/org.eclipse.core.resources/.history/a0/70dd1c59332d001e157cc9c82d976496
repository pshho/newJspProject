package projClass;

public class Star {

	public static void main(String[] args) {
		for(int i=0; i<5; i++) {
			for(int j=0; j<10-i; j++) {
				System.out.print(" ");
				// System.out.print(j);
			}
			
			for(int j=0; j<i+1; j++) {
				System.out.print("☆");
				// System.out.print("두");
			}
			
			System.out.println();
		}
	}

}
