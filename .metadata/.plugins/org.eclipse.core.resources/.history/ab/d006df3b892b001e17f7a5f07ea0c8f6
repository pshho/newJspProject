package basic_P;

public class ThreeSixNine2 {
	public static void main(String[] args) {
		int n = 20;
		for(int i=1; i<=n; i++){
			int[] arr = new int[i];
			int a = i%3;
			
			try {
				int b = i/a;
			}catch(ArithmeticException e) {
				System.out.println(i);
				System.out.println(b);
			}catch(ArrayIndexOutOfBoundsException e) {
				
			}
		}
	}
}
