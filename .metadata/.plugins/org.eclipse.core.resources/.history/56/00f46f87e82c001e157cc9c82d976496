package projClass;

public class Major{	// 과목 클래스
	private String[] majors;	// 과목
	private int[] grades;		// 점수
	
	// 과목 생성자
	public Major(String... majors) {
		this.majors = majors;
	}
	
	public Major(int... grades) {
		this.grades = grades;
	}

	public String[] getMajors() {
		return majors;
	}
}
